<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div id="mainPicContainer">
        <img class="bottom" src="http://assets.feministing.com/wp-content/uploads/2016/01/books.jpg" />
        <img class="top" src="http://www.uglydogbooks.com/wp-content/uploads/2014/04/books.jpg" />
    </div>
    <div id="topMenu">
        <div class="topMenuBtnLeft"><a href="/main/Index">Home</a></div>
        <div class="topMenuBtnLeft"><a href="/loan/details?username=@Session["personid"]">My Loans</a></div>
        <div class="topMenuBtnLeft"><a href="/account/Index">Accounts</a></div>
        <div class="topMenuBtnLeft"><a href="/search/Index">Search</a></div>
        @if(Session.Count == 0 ||Session["level"].ToString() == "2")
        {
        <div class="topMenuBtnLeft"><a href="/COPYSets/Index">Books </a></div>
        }
        @*else
        {
            <div class="topMenuBtnLeft"><a href="/COPYSets/Index">Books </a></div>
        }*@

        <div class="topMenuBtnRight"><a href="/login/Logout">Log Out</a></div>
    </div>


    <div id="bodyContent">
        @RenderBody()

    </div>

    <footer></footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
